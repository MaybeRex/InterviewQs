{
  "extends": "airbnb-base",
  "plugins": [
    "html",
  ],
  "globals": {
    "document": true,
    "axios": true,
  },
  "env": {
    "browser": true,
  },
  "rules": {
    "no-param-reassign": 0,
    "no-continue": 0,
    "for-direction": 0,
    "no-mixed-operators": 0,
    "no-restricted-globals": 0,
    "no-unused-expressions": 0,
  },
}
